<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
  </configSections>
  <!-- アプリケーション設定を定義する（あらかじめ用意された標準の設定項目とは別に、アプリケーション固有の管理項目を定義できる） -->
  <appSettings>
    <!-- p.119 [Add] 控えめな検証を無効化
    <add key="ValidationSettings:UnobtrusiveValidationMode" value="None"/>  
	-->
    <!-- p.145 [Add] データベース接続情報 (cf p.149～152)  ←？？？ -->
    <add key="ChartImageHandler" value="storage=file;timeout=20;dir=c:\TempImageFiles\;" />
    <!-- p.553 [Add] -->
    <add key="Author" value="山田太郎" />
  </appSettings>
  <!-- WEBサーバー(IIS)に関わる設定 -->
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <handlers>
      <remove name="ChartImageHandler" />
      <add name="ChartImageHandler" preCondition="integratedMode" verb="GET,HEAD,POST" path="ChartImg.axd" type="System.Web.UI.DataVisualization.Charting.ChartHttpHandler, System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      <!-- p.539 [Add] 特定の拡張子へのアクセスを禁止する -->
      <add name="HttpForbiddenHandler" verb="*" path="*.txt" type="System.Web.HttpForbiddenHandler" />
      <!-- p.544 [Add] 自作のHTTPハンドラーを登録 -->
      <add name="SourceOutputHandler" verb="GET" path="*.src" type="SelfAspNet.Chap10.SourceOutputHandler" />
    </handlers>
    <!-- p.536 [Add] HTTPモジュールの定義 
	<modules>
	  <add name="LoggingModule" type="SelfAspNet.Chap10.LoggingModule"/>
	</modules>
	-->
  </system.webServer>
  <connectionStrings>
    <add name="SelfAsp" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=|DataDirectory|\SelfAsp.mdf;Integrated Security=True" providerName="System.Data.SqlClient" />
    <!-- p.283 [Add] データモデル（Entity Data Model -->
    <add name="SelfAspEntities" connectionString="data source=(LocalDB)\MSSQLLocalDB;attachdbfilename=|DataDirectory|\SelfAsp.mdf;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <!-- アプリケーションの構成 -->
  <system.web>
    <httpHandlers>
      <add path="ChartImg.axd" verb="GET,HEAD,POST" type="System.Web.UI.DataVisualization.Charting.ChartHttpHandler, System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" validate="false" />
    </httpHandlers>
    <pages>
      <controls>
        <add tagPrefix="asp" namespace="System.Web.UI.DataVisualization.Charting" assembly="System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
      </controls>
    </pages>
    <compilation targetFramework="4.8">
      <assemblies>
        <add assembly="System.Web.DataVisualization, Version=4.0.0.0, Culture=neutral, PublicKeyToken=31BF3856AD364E35" />
      </assemblies>
    </compilation>
    <!-- p.89 [Add] executionTimeout属性, maxRequestLength属性を追加-->
    <httpRuntime targetFramework="4.8" executionTimeout="110" maxRequestLength="4096" />
    <!-- p.110 [Add] -->
    <!-- <pages controlRenderingCompatibilityVersion="3.5" /> -->
    <!-- p.327 [Add] アプリケーション全体でトレース機能を有効
    <trace enabled="true" localOnly="true" pageOutput="true" traceMode="SortByTime"/>
    -->
    <!-- p.342 [Add] クッキーのデフォルト設定を宣言する -->
    <httpCookies httpOnlyCookies="true" requireSSL="false" />
    <!-- p.348 [Add] State Serviceを有効化する -->
    <!-- <sessionState mode="StateServer" stateConnectionString="tcpip=127.0.0.1:42424" /> -->
    <!-- p.350 [Add] SQL Serverを有効化する -->
    <sessionState mode="SQLServer" sqlConnectionString="Data Source=(LocalDB)\MSSQLLocalDB;Integrated Security=True" />
    <!-- p.363 [Add] ページキャッシュの共通ルールを定義する -->
    <caching>
      <outputCacheSettings>
        <outputCacheProfiles>
          <add name="MyCache" enabled="true" duration="120" varyByParam="*" />
        </outputCacheProfiles>
      </outputCacheSettings>
      <!-- p.365 [Add] キャッシュ対象となるデータベースをアプリケーションに登録 -->
      <sqlCacheDependency enabled="true" pollTime="5000">
        <databases>
          <add name="MyCache" connectionStringName="SelfAsp" />
        </databases>
      </sqlCacheDependency>
    </caching>
    <!-- p.435 [Add] サイト内に複数のサイトマップファイルを配置する -->
    <siteMap enabled="true" defaultProvider="DefaultSiteMapProvider">
      <providers>
        <add name="DefaultSiteMapProvider" type="System.Web.XmlSiteMapProvider" siteMapFile="Web.sitemap" />
        <add name="NextSiteMapProvider" type="System.Web.XmlSiteMapProvider" siteMapFile="Web2.sitemap" />
        <!-- p.446 [Add] サイトマッププロバイダーをアプリケーションに登録する -->
        <add name="SqlSiteMapProvider" type="SelfAspNet.Chap09.SqlSiteMapProvider" connectName="SelfAsp" />
      </providers>
    </siteMap>
    <!-- p.521 [Add] カスタムエラーページの設定 
    <customErrors mode="On" defaultRedirect="~/Chap10/Error.html">
    </customErrors>
	-->
  </system.web>
